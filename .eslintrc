{
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "modules": true
        }
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off"
    },
    "env": {
        "browser": true,
        "node": true
    }
}
